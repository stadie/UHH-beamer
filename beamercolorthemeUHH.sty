\ProvidesPackage{beamercolorthemeUHH}
  [2014/08/19 v1.0 beamer 'UHH' colour theme]

\RequirePackage{colortbl}

\mode<presentation>

\definecolor{UHHlightgrey}{RGB}{136,136,136}
\definecolor{UHHgrey}{RGB}{110,110,110}
\definecolor{UHHred}{RGB}{216,0,21}
\definecolor{UHHdarkred}{RGB}{206,44,72}

\setbeamercolor{normal text}{fg=black}
\setbeamercolor{alerted text}{fg=UHHred}
%\setbeamercolor{example text}{fg=blue}
\setbeamercolor{structure}{fg=UHHlightgrey}
\setbeamercolor{title}{fg=black}
\setbeamercolor{author}{fg=black}
\setbeamercolor{date}{fg=black}
\setbeamercolor{framesubtitle}{fg=UHHgrey}
\setbeamercolor{headline}{fg=black}
\setbeamercolor{footline}{fg=white}
\setbeamercolor{frametitle}{fg=black}
\setbeamercolor{title in head/foot}{fg=white}
\setbeamercolor{author in head/foot}{fg=white}



%from orchid color theme
\setbeamercolor{block title}{use=structure,fg=white,bg=UHHlightgrey}
\setbeamercolor{block title alerted}{use=alerted text,fg=white,bg=alerted text.fg!75!black}
\setbeamercolor{block title example}{use=example text,fg=white,bg=example text.fg!75!black}

\setbeamercolor{block body}{parent=normal text,use=block title,bg=block title.bg!10!bg}
\setbeamercolor{block body alerted}{parent=normal text,use=block title alerted,bg=block title alerted.bg!10!bg}
\setbeamercolor{block body example}{parent=normal text,use=block title example,bg=block title example.bg!10!bg}

\mode<all>